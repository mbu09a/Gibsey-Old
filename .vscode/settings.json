{
    "files.exclude": {
        "**/.venv/**": true,
        "**/.venv": true,
        "**/__pycache__": true,
        "**/*.pyc": true,
        "**/*.pyo": true,
        "**/*.pyd": true,
        "**/*.so": true,
        "**/*.dll": true,
        "**/*.dylib": true,
        "**/*.obj": true,
        "**/*.o": true,
        "**/*.a": true,
        "**/*.lib": true,
        "**/*.out": true,
        "**/*.app": true,
        "**/*.exe": true,
        "**/*.msi": true,
        "**/*.msm": true,
        "**/*.msp": true,
        "**/node_modules": true
    },
    "python.analysis.extraPaths": ["./apps/backend/app"],
    "python.analysis.typeCheckingMode": "basic"
}
